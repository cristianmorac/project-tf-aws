    agent any
    stages {
        stage('init'){
            steps {
                echo 'Initializing...'
            }
        }
        stage('test'){
            steps {
                echo 'Running tests...'
            }
        }
        stage('build'){
            steps {
                echo 'Building...'
            }
        }
        stage('deploy'){
            steps {
                echo 'Deploying...'
            }
        }
    }
}